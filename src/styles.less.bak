@import "../node_modules/bootstrap/dist/css/bootstrap.css";
@import '../node_modules/material-icons/iconfont/material-icons.css';
@import '~@angular/material/prebuilt-themes/deeppurple-amber.css';

@color-weekend: red;
@main-color: #cf0017;
@submain-color: #5EC300;

